pipeline {
  agent any
  stages {
    stage('vault password creds') {
     environment {
        vault_pass = credentials('my_jenkinsfile_vault1')
      }
      steps {
        sh "echo 'My secret file is $vault_pass'"
      }
    }
    stage('build Device configuration') {
      steps {
        sh 'ansible-playbook pb_arista_fabric_deploy_01.yml --tags mgmt'
       }
      }
    } 
}